# Server Configuration
PORT=3000

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Cache Settings
CACHE_TTL=300000 # Cache Time-to-Live in milliseconds (default: 5 minutes)

# Security Configurations
RATE_LIMIT_POINTS=100 # Maximum number of requests allowed
RATE_LIMIT_DURATION=60 # Time window for rate limiting in seconds
RATE_LIMIT_BLOCK=300 # Block duration after exceeding rate limit in seconds

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# User Agent
USER_AGENT=WhiteHouseActionsTracker/1.0
